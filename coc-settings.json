		"latex": {
			"command": "/usr/bin/texlab",
			"ignoredRootPaths": ["~"],
			"filetypes": ["tex", "bib", "plaintex", "context"]
		},
		"lua": {
			"command": "lua-lsp",
			"filetypes": ["lua"]
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"],
			"initializationOptions": {
				"gocodeCompletionEnabled": true,
				"diagnosticsEnabled": true,
				"lintTool": "golint"
			}
		},
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": [""]
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
			"rootPatterns": [
				".ccls",
				".ccls-root",
				"compile_commands.json",
				".git/",
				".hg/"
			],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			}
		}
